pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "Building the project..."'
               
            }
        }
        
        stage('Test') {
            steps {
                sh 'echo "Running unit tests..."'
                
            }
        }
        
        stage('Deploy') {
            steps {
                sh 'echo "Deploying the artifacts..."'
          
            }
        }
    }
}
